apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: preferences-ingress
  namespace: gpii
spec:
  tls:
    - hosts:
      - preferences.<%= ENV["TF_VAR_environment"] %>.gpii.net
      secretName: preferences-tls
  rules:
    - host: preferences.<%= ENV["TF_VAR_environment"] %>.gpii.net
      http:
        paths:
        - path: /
          backend:
            serviceName: preferences
            servicePort: 80